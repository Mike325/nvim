{
  "ycm_extra_conf.py"             : {"type": "DefaultYCM"},
  "tests/test.sh"                 : {"type": "Tests"},
  "tests/*"                       : {"type": "Tests"},
  "lua/tests/*_spec.lua"          : {"type": "Tests", "alternate" : [
    "lua/utils/{}.lua",
    "lua/completions/{}.lua",
    "lua/configfiles/{}.lua",
    "lua/filetypes/{}.lua",
    "lua/git/{}.lua",
    "lua/jobs/{}.lua",
    "lua/neovim/{}.lua",
    "lua/plugins/{}.lua",
    "lua/storage/{}.lua",
    "lua/sys/{}.lua",
    "lua/{}.lua"
  ]},
  "init.lua"                      : {"type": "Init"},
  "ginit.vim"                     : {"type": "Ginit", "alternate" : "init.lua"},
  ".luacheckrc"                   : {"type": "Lua"},
  "lua/*.lua"                     : {"type": "Lua", "alternate"   : "lua/tests/{}_spec.lua"},
  "lua/plugins/snippets/init.lua" : {"type": "Snippets"},
  "lua/plugins/snippets/*.lua"    : {"type": "Snippets"},
  "lua/utils/init.lua"            : {"type": "Utils"},
  "lua/utils/*.lua"               : {"type": "Utils", "alternate" : "lua/tests/{}_spec.lua"},
  "python3/*.py"                  : {"type": "Python3"},
  "python/*.py"                   : {"type": "Python"},
  "autoload/*.vim"                : {"type": "Autoload"},
  "autoload/plugins/*.lua"        : {"type": "PluginsAutoload"},
  "syntax/*.vim"                  : {"type": "Syntax"},
  "compiler/*.lua"                : {"type": "Compiler"},
  "plugin/*.lua"                  : {"type": "Plugin"},
  "ftdetect/*.vim"                : {"type": "FtDetect"},
  "ftplugin/*.lua"                : {"type": "FtPlugin"},
  "rplugins/python3/*.py"         : {"type": "RPython3"},
  "rplugins/python/*.py"          : {"type": "RPython"},
  "after/plugin/*.lua"            : {"type": "PluginAfter"},
  "after/ftplugin/*.lua"          : {"type": "FtPluginAfter"},
  "after/syntax/*.lua"            : {"type": "SyntaxAfter"},
  "after/compiler/*.lua"          : {"type": "CompilerAfter"},
  "spell/*.add"                   : {"type": "Spells"},
  "setting.toml"                  : {"type": "Gonvim"},
  "skeletons/*"                   : {"type": "Skeletons"}
}
